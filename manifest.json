{
  "short_name": "Youtube URL Scanner",
  "name": "Youtube URL Scanner",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "build/index.html",
    "default_icon": {
      "16": "/images/website.png",
      "32": "/images/website.png",
      "48": "/images/website.png",
      "128": "/images/website.png"
    }
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "/images/website.png",
    "32": "/images/website.png",
    "48": "/images/website.png",
    "128": "/images/website.png"
  }
}
